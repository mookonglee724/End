<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.ContactDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.ContactEntity" id="contactMap">
        <result property="name" column="name"/>
        <result property="phone" column="phone"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.ContactVO" >
		SELECT * FROM contact  contact
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.ContactVO" >
		SELECT  contact.* FROM contact  contact
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.ContactView" >

		SELECT  contact.* FROM contact  contact
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.ContactView" >
		SELECT * FROM contact  contact <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
</mapper>